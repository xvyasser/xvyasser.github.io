table _Measures
	lineageTag: 26d1b3f9-0c38-4bdf-8610-26d76cb97f66

	measure TotalEmployees = DISTINCTCOUNT(DimEmployee[EmployeeID])
		formatString: 0
		lineageTag: e870c0f2-2310-4cf2-8450-1e3a02ffdf25

	measure ActiveEmployees = CALCULATE([TotalEmployees], FILTER(DimEmployee, DimEmployee[Attrition] = "No"))
		formatString: 0
		lineageTag: ce46b07a-ffc1-4185-9288-510a6afd20ce

	measure InactiveEmployees = CALCULATE([TotalEmployees], FILTER(DimEmployee, DimEmployee[Attrition] = "Yes"))
		formatString: 0
		lineageTag: 2f75e474-ad62-4549-aeb0-8cbc4b21148d

	measure '% Attrition Rate' = DIVIDE([InactiveEmployees], [TotalEmployees])
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: c96f46fa-4e9a-4b4c-9ce6-07befaaae065

		changedProperty = FormatString

	measure TotalEmployeesDate =
			
			CALCULATE (
			    [TotalEmployees],
			    USERELATIONSHIP ( DimEmployee[HireDate], DimDate[Date] )
			)
		formatString: 0
		lineageTag: 67cfe081-dce0-481b-abfc-fafd0232ead8

	measure AverageSalary = AVERAGE(DimEmployee[Salary])
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: b11ab4a8-d32d-4543-bf01-f00bbef491ca

		changedProperty = FormatString

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	measure LastReviewDate =
			
			IF (
			    MAX ( FactPerformanceRating[ReviewDate] ) = BLANK (),
			    "No Review Yet",
			    MAX ( FactPerformanceRating[ReviewDate] )
			)
		formatString: mm/dd/yyyy
		lineageTag: 1755468a-5030-4829-9c94-f246ee33340b

		changedProperty = FormatString

		annotation PBI_FormatHint = {"isCustom":true}

	measure NextReviewDate =
			
			VAR reviewOrHire =
			    IF (
			        MAX ( FactPerformanceRating[ReviewDate] ) = BLANK (),
			        MAX ( DimEmployee[HireDate] ),
			        MAX ( FactPerformanceRating[ReviewDate] )
			    )
			RETURN
			    reviewOrHire + 365
		formatString: Mm/dd/yyyy
		lineageTag: 46b8dfe2-a172-4548-9f7f-0cb6513c2c04

		changedProperty = FormatString

		annotation PBI_FormatHint = {"isCustom":true}

	measure JobSatisfaction = MAX(FactPerformanceRating[JobSatisfaction])
